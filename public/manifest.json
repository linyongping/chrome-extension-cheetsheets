{
  "manifest_version": 2,
  "name": "FE cheatsheets",
  "version": "1.0",
  "description": "FE helper",
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["/css/content.css"],
      "js": ["/js/content.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ]
